{
    "name": "cashfree/cashfree-pg-sdk-php",
    "description": "Cashfree Payment Gateway Framework for PHP",
    "keywords": [
        "php",
        "sdk",
        "rest",
        "api"
    ],
    "homepage": "https://www.cashfree.com",
    "license": "unlicense",
    "authors": [
        {
            "name": "Cashfree",
            "homepage": "https://www.cashfree.com"
        }
    ],
    "require": {
        "php": "^7.3 || ^8.0",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^7.3",
        "guzzlehttp/psr7": "^1.7 || ^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0 || ^9.0",
        "friendsofphp/php-cs-fixer": "^2.12"
    },
    "autoload": {
        "psr-4": { "OpenAPI\\Client\\" : "lib/" }
    },
    "autoload-dev": {
        "psr-4": { "OpenAPI\\Client\\Test\\" : "test/" }
    }
}
